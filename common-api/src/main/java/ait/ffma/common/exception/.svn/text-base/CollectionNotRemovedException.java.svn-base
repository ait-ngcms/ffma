package ait.ffma.common.exception;

/**
 * Exception thrown when a database collection cannot be removed.
 */
public class CollectionNotRemovedException extends FfmaFunctionalException {

	private static final long serialVersionUID = -7288167703171144524L;

	/**
	 * @param message
	 * 		The message for the exception
	 * @param excep
	 * 		The exception that prompted the creation of this exception
	 */
	public CollectionNotRemovedException(String message, Throwable excep) {
		super(message, excep);
	}
	
	/**
	 * @param message
	 * 		The message for the exception
	 */
	public CollectionNotRemovedException(String message) {
		super(message);
	}
	
}
