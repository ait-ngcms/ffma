package ait.ffma.common.exception;

/**
 * Exception thrown when a DigitalObject cannot be updated in the Data Registry.
 */
public class ObjectNotUpdatedException extends FfmaFunctionalException{

	private static final long serialVersionUID = -4584252067316670118L;

	/**
	 * @param message
	 * 		The message for the exception
	 * @param excep
	 * 		The exception that prompted the creation of this exception
	 */
	public ObjectNotUpdatedException(String message, Throwable excep) {
		super(message, excep);
	}
	
	/**
	 * @param message
	 * 		The message for the exception
	 */
	public ObjectNotUpdatedException(String message) {
		super(message);
	}
	
	
}
